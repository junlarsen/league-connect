  "bugs": {
    "url": "https://github.com/matsjla/league-connect/issues"
  },
  "homepage": "https://github.com/matsjla/league-connect#readme",
  "dependencies": {
    "@types/node-fetch": "^2.6.2",
    "@types/ws": "^8.5.3",
    "node-fetch": "^2.6.7",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^14.14.7",
    "jest": "^28.1.0",
    "nugget": "^2.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.3",
    "tsup": "^6.1.3",
